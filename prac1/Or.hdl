 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(a=a, out=na);
    Not(a=b, out=nb);
    And(a=na, b=nb, out=andOut);
    Not(a=andOut, out=out);
}

